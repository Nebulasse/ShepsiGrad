{
  "name": "shepsigrad",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --no-dev --minify",
    "android": "expo start --android --no-dev --minify",
    "ios": "expo start --ios --no-dev --minify",
    "web": "expo start --web",
    "frontend": "cd frontend && npm start",
    "backend": "cd backend && npm start",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "clean": "expo start --clear",
    "reset": "rm -rf node_modules/.cache && watchman watch-del-all && expo start -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
