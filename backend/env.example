# Основные настройки приложения
NODE_ENV=production
PORT=3000
API_PREFIX=/api
CORS_ORIGIN=*

# Supabase (ОБЯЗАТЕЛЬНО)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key

# JWT настройки
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# Email настройки (опционально)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-app-password
EMAIL_FROM=noreply@rentalapp.com

# Хранилище файлов
STORAGE_PROVIDER=local
UPLOAD_DIR=./uploads

# S3 настройки (если используете S3 вместо MinIO)
S3_BUCKET=rental-app
S3_REGION=eu-central-1
S3_ACCESS_KEY=your-s3-access-key
S3_SECRET_KEY=your-s3-secret-key
S3_ENDPOINT=

# MinIO настройки (если используете MinIO)
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_BUCKET=rental-app
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_USE_SSL=false

# Логирование
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# Платежи (ЮKassa)
YOOKASSA_SHOP_ID=your-shop-id
YOOKASSA_SECRET_KEY=your-secret-key
PAYMENT_RETURN_URL=http://localhost:3000/payment/success

# Google Maps API (опционально)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Устаревшие настройки (не нужны с Supabase)
# DATABASE_URL=postgresql://username:password@localhost:5432/rental_app
# DB_HOST=localhost
# DB_PORT=5432
# DB_USERNAME=postgres
# DB_PASSWORD=your_password
# DB_NAME=rental_app
# DB_SSL=false
